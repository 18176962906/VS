@model WebApplication1.ViewModels.EmployeeListViewModels

@{
    Layout = "~/Views/Shared/MyLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h2>@Model.UserName,@Model.Greeting</h2>
    <p><a href="/Employee/CreateEmployee">新增</a></p>
    <form action="/employee/Search" method="post">
        关键字：<input type="text" name="searchString" value=""><br />
        <input type="submit" value="搜索">
    </form>
    <div> 
        <ul>
        @foreach (var item in Model.EmployeeViewList)
        {
           <li>@item.EmployeeName,@item.EmployeeSalary,@item.EmployeeGrade<a href="/employee/delete/@item.EmloyeeId">  删除  </a><a href="/employee/Edit/@item.EmloyeeId">  编辑  </a></li>
        }
        </ul>
    </div>
</body>
</html>
